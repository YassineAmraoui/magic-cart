{
    "name": "Magic Cart",
    "manifest_version": 2,
    "version": "0.2",
    "description": "add missing add to cart button",
    "permissions": [],
    "icons": {
        "128": "icons/icon_128.png"
    },
    "content_scripts": [
        {
            "js": [
                "ldlc.js"
            ],
            "matches": [
                "*://*.ldlc.com/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "amd.js"
            ],
            "matches": [
                "*://*.amd.com/*"
            ],
            "run_at": "document_end"
        }
    ]
}