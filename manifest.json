    "version": "0.1",
    "description": "add missing add to cart button",
    "permissions": [],
    "icons": {
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "content_scripts": [
        {
            "js": [
                "ldlc.js"
            ],
            "matches": [
                "*://*.ldlc.com/*"
